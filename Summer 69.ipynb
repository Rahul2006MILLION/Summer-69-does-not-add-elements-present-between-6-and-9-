w=int(input("Enter the Size of List you Want to Create: "))
inside=False
s=0
l=[]
for i in range(w):
    a=int(input("Enter the Number: "))
    l.append(a)
for j in l:
    if j==6:
        inside=True
    elif j==9 and inside==True:
        inside=False
    elif inside==False:
        s+=j
